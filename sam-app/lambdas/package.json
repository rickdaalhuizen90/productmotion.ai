{
  "name": "hello_world",
  "version": "1.0.0",
  "description": "hello world sample for NodeJS",
  "main": "dist/app.js",
  "repository": "https://github.com/awslabs/aws-sam-cli/tree/develop/samcli/local/init/templates/cookiecutter-aws-sam-hello-nodejs",
  "author": "SAM CLI",
  "license": "MIT",
  "scripts": {
    "unit": "jest",
    "lint": "eslint 'src/*.ts' --quiet --fix",
    "compile": "npx tsc --project tsconfig.json",
    "watch": "npx tsc --watch --project tsconfig.json",
    "test": "pnpm run compile && pnpm run unit",
    "bundle": "esbuild src/app.ts --bundle --platform=node --target=es2020 --outdir=dist",
    "dynamodb": "./node_modules/.bin/dynamodb-admin --dynamo-endpoint=http://localhost:8000"
  },
  "dependencies": {
    "@aws-sdk/client-dynamodb": "^3.786.0",
    "@aws-sdk/lib-dynamodb": "^3.786.0",
    "aws-lambda": "^1.0.7",
    "axios": "^1.8.4",
    "esbuild": "^0.14.14"
  },
  "devDependencies": {
    "@jest/globals": "^29.2.0",
    "@types/aws-lambda": "^8.10.149",
    "@types/jest": "^29.2.0",
    "@types/node": "^20.5.7",
    "@typescript-eslint/eslint-plugin": "^8.29.1",
    "@typescript-eslint/parser": "^8.29.1",
    "dynamodb-admin": "^5.1.3",
    "eslint": "^9.24.0",
    "eslint-config-prettier": "^10.1.2",
    "eslint-plugin-prettier": "^5.2.6",
    "jest": "^29.2.1",
    "prettier": "^3.5.3",
    "ts-jest": "^29.0.5",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.4"
  }
}
